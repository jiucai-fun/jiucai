<?xml version="1.0" encoding="UTF-8" standalone="no" ?>

<!-- native为内部消息，common是公共消息每个模块都能使用 -->
<protocols>
    <module id="1" name="native">
        <protocol location="com.zfoo.net.router.attachment.SignalAttachment"/>
    </module>

    <module id="2" name="common">
        <protocol location="com.zfoo.net.packet.common.Message"/>
        <protocol location="com.zfoo.net.packet.common.Error"/>
        <protocol location="com.zfoo.net.packet.common.Heartbeat"/>
        <protocol location="com.zfoo.net.packet.common.Ping"/>
        <protocol location="com.zfoo.net.packet.common.Pong"/>
    </module>

    <module id="3" name="spider">
        <protocol id="1000" location="fun.jiucai.packet.SimulatorRegisterAsk"/>
        <protocol id="1001" location="fun.jiucai.packet.SimulatorRegisterAnswer"/>
        <protocol id="1010" location="fun.jiucai.packet.SimulatorChatAsk"/>
        <protocol id="1011" location="fun.jiucai.packet.SimulatorChatAnswer"/>
        <protocol id="1021" location="fun.jiucai.packet.SimulatorStatusAsk"/>
        <protocol id="1100" location="fun.jiucai.packet.ChatBotRequest"/>
        <protocol id="1101" location="fun.jiucai.packet.ChatBotNotice"/>
        <protocol id="1102" location="fun.jiucai.packet.ChatBotRegisterRequest"/>
        <protocol id="1103" location="fun.jiucai.packet.ChatBotRegisterResponse"/>
        <protocol id="1200" location="fun.jiucai.packet.ClipboardLockAsk"/>
        <protocol id="1201" location="fun.jiucai.packet.ClipboardLockAnswer"/>
        <protocol id="1202" location="fun.jiucai.packet.ClipboardUnlockAsk"/>
        <protocol id="1203" location="fun.jiucai.packet.ClipboardUnlockAnswer"/>
    </module>

</protocols>
